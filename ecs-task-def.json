{
  "family": "strapi-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::145065858967:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "strapi",
      "image": "145065858967.dkr.ecr.ap-south-1.amazonaws.com/my-strapi-project-vivek",
      "portMappings": [
        {
          "containerPort": 1337
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/strapi/mayank",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}